{
  "name": "@liquid-state/pathways-client",
  "version": "0.0.1",
  "description": "Client library for Pathways Service",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn format && tsc",
    "format": "prettier --parser typescript --single-quote --trailing-comma es5 --print-width 99 --write \"./src/**/*.ts\"",
    "test": "jest",
    "prepack": "jest && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liquid-state/pathways-client.git"
  },
  "author": "Liquid State <dev@liquid-state.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liquid-state/pathways-client/issues"
  },
  "homepage": "https://github.com/liquid-state/pathways-client#readme",
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|.*.test)\\.(ts|tsx|js)$",
    "roots": [
      "<rootDir>/src"
    ]
  }
}
